<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gr.qrapi.core.model.Contact" table="CONTACT">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="firstName" type="java.lang.String">
            <column name="FIRSTNAME" />
        </property>
        <property name="lastName" type="java.lang.String">
            <column name="LASTNAME" />
        </property>
        <property name="emailAddress" type="java.lang.String">
            <column name="EMAILADDRESS" />
        </property>
        <property name="gender" type="java.lang.String">
            <column name="GENDER" />
        </property>
        <property name="phoneNumber" type="java.lang.String">
            <column name="PHONENUMBER" />
        </property>
        <property name="status" type="java.lang.Boolean">
            <column name="STATUS" />
        </property>
        <bag name="addresses" inverse="false" table="ADDRESS" lazy="false">
            <key>
                <column name="ID" />
            </key>
            <one-to-many class="com.gr.qrapi.core.model.Address" />
        </bag>
        <many-to-one name="account" class="com.gr.qrapi.core.model.Account" fetch="join">
            <column name="ACCOUNT" />
        </many-to-one>
    </class>
</hibernate-mapping>
