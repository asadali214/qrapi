<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gr.qrapi.core.model.Address" table="Address">
		<id column="id" name="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="street_address" name="streetAddress"
			type="java.lang.String" />
		<property column="city" name="city"
			type="java.lang.String" />
		<property column="state" name="state"
			type="java.lang.String" />
		<property column="country" name="country"
			type="java.lang.String" />
	</class>

</hibernate-mapping>

