<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gr.qrapi.core.model.AlertLocation" table="ALERTLOCATION">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="city" type="java.lang.String">
            <column name="CITY" />
        </property>
        <property name="country" type="java.lang.String">
            <column name="COUNTRY" />
        </property>
        <many-to-one name="alert" class="com.gr.qrapi.core.model.AlertProfile" fetch="join">
            <column name="ALERT" />
        </many-to-one>
    </class>
</hibernate-mapping>
