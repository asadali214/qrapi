<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gr.qrapi.core.model.AlertProfile" table="ALERTPROFILE">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <many-to-one name="account" class="com.gr.qrapi.core.model.Account" fetch="join">
            <column name="ACCOUNT" />
        </many-to-one>
        <bag name="locations" table="ALERTLOCATION" inverse="false" lazy="false">
            <key>
                <column name="ID" />
            </key>
            <one-to-many class="com.gr.qrapi.core.model.AlertLocation" />
		</bag>
    </class>
</hibernate-mapping>
